generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Reservation {
  id          String   @id @default(uuid())
  todoID      String
  todoContent String
  edit        Boolean  @default(false)
  pw          String
  person      String
  rooms       String
  startTime   String
  finishTime  String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}